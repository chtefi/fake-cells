  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-eslint": "^3.1.15",
    "babel-loader": "^5.1.4",
    "chai": "^3.2.0",
    "eslint": "^0.23",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-react": "^2.3.0",
    "isparta": "^3.0.3",
    "istanbul": "^0.3.17",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.9",
